{
  "name": "robot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "webpack": "webpack --progress",
    "serve": "webpack-dev-server --host localhost --port 3000 --inline --progress",
    "compile": "tsc --rootDir src --outDir wwwroot",
    "delete": "del wwwroot\\*.* /q /s",
    "distro": "npm run compile && npm run webpack",
    "test_console": "mocha --require ts-node/register src/app/tests/**/*.spec.ts",
    "test_Mochawesome": "mocha --require ts-node/register src/app/tests/**/*.spec.ts --reporter mochawesome"
  },
  "dependencies": {
    "@angular/animations": "~7.0.0-beta.0",
