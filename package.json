    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~2.30.1",
    "image-webpack-loader": "~4.3.1",
    "lodash": "~4.17.10",
    "mocha": "~5.2.0",
    "mochawesome": "~3.0.2",
    "raw-loader": "~0.5.1",
    "resolve-url-loader": "~2.2.1",
