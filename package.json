    "@angular/forms": "~7.0.0-beta.0",
    "@angular/http": "~7.0.0-beta.0",
    "@angular/platform-browser": "~7.0.0-beta.0",
    "@angular/platform-browser-dynamic": "~7.0.0-beta.0",
    "@angular/router": "~7.0.0-beta.0",
    "core-js": "~2.5.7",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.2.2",
    "zone.js": "~0.8.20"
  },
  "devDependencies": {
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@types/chai": "~4.1.4",
